{
	"id": "somehow-digital",
	"name": "somehow.digital Dictionary",
	"description": "Shared dictionary for somehow.digital projects.",
	"readonly": true,
	"import": ["@cspell/dict-de-de"],
	"dictionaries": [
		"en_US",
		"de-de",
		"softwareTerms",
		"typescript",
		"node",
		"html",
		"css",
		"bash",
		"misc",
		"filetypes",
		"fonts",
		"data-science",
		"somehow-digital/general",
		"somehow-digital/names"
	],
	"dictionaryDefinitions": [
		{
			"name": "somehow-digital/general",
			"path": "./general.txt",
			"description": "General words and phrases."
		},
		{
			"name": "somehow-digital/names",
			"path": "./names.txt",
			"description": "Names of projects, libraries and companies."
		}
	],
	"ignorePaths": [
		"**/node_modules/",
		"**/.git/",
		"**/.husky/",
		"**/.idea/",
		"**/CODEOWNERS",
		"**/package.json",
		"**/pnpm-lock.yaml",
		"**/tsconfig.json",
		"**/.npmrc",
		"**/.nvmrc",
		"**/.gitignore",
		"**/.gitattributes",
		"**/.editorconfig",
		"**/.prettierignore",
		"**/*.tsbuildinfo"
	]
}
